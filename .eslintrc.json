{
    
    "extends": [
        "airbnb",
        "prettier"
    ],
    "parserOptions": {
        "sourceType": "module",
        "allowImportExportEverywhere": true,
        "ecmaVersion": 12
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "no-undef":"warn",
        "func-names":"off",
        "no-unused-vars":"warn",
        "no-console": "off",
        "react/react-in-jsx-scope": "off",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}
